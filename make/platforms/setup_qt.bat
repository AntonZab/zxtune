@ECHO OFF

SET | FIND "QT_VERSION" > NUL && GOTO NoDefQt
FOR /F %%V in (%BUILD_TOOLS_DIR%\qt-%TOOLSET%-%BUILD_ARCH%) DO SET QT_VERSION=%%V
ECHO Using qt version %QT_VERSION%
:NoDefQt
SET QT_DIR=%BUILD_TOOLS_DIR%\qt-%QT_VERSION%-%TOOLSET%-%BUILD_ARCH%

ECHO %PATH% | FIND "%QT_DIR%" > NUL && GOTO Quit

SET PATH=%QT_DIR%\bin;%PATH%
SET LIB=%QT_DIR%\lib;%LIB%
SET INCLUDE=%QT_DIR%\include;%INCLUDE%
:Quit
